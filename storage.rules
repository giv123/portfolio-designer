rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /project-images/{allPaths=**} {
      allow read: if true;
      allow write, delete: if request.auth != null;
    }
    match /project-thumbnails/{allPaths=**} {
      allow read: if true;
      allow write, delete: if request.auth != null;
    }
    match /carousel-desktop-images/{allPaths=**} {
      allow read: if true;
      allow write, delete: if request.auth != null;
    }
    match /carousel-mobile-images/{allPaths=**} {
      allow read: if true;
      allow write, delete: if request.auth != null;
    }
  }
}
